<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,800,700,400italic,600italic,700italic,800italic,300italic" rel="stylesheet" type="text/css">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")    

    <link rel="stylesheet" type="text/css" href="/assets/css/bootflat.min.css" />
    <script src="/Scripts/angular.js"></script>
    <script src="/assets/app/global.js"></script>
    <script src="/assets/app/Member/Controller/memberController.js"></script>
    <script src="/assets/app/Member/Factory/memberFactory.js"></script>
    <script src="/assets/app/Member/Directive/memberDirective.js"></script>
</head>
<body>

    <div class="container body-content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
